использовать "стд/ввод_вывод" как ио;

ио.вывести("=== Тест словарей ===");

// Тест 1: Пустой словарь
ио.вывести("Тест 1: Пустой словарь");
пер пустой = {};
ио.вывести(длина(пустой));  // 0

// Тест 2: Словарь со строковыми ключами
ио.вывести("Тест 2: Строковые ключи");
пер персона = {
    "имя": "Иван",
    "возраст": 25,
    "город": "Москва"
};
ио.вывести(длина(персона));       // 3
ио.вывести(персона["имя"]);        // "Иван"
ио.вывести(персона["возраст"]);    // 25

// Тест 3: Словарь с числовыми ключами
ио.вывести("Тест 3: Числовые ключи");
пер номера = {
    1: "один",
    2: "два",
    3: "три"
};
ио.вывести(номера[1]);  // "один"
ио.вывести(номера[2]);  // "два"
ио.вывести(номера[3]);  // "три"

// Тест 4: Смешанные типы значений
ио.вывести("Тест 4: Смешанные значения");
пер данные = {
    "текст": "привет",
    "число": 42,
    "флаг": истина
};
ио.вывести(данные["текст"]);   // "привет"
ио.вывести(данные["число"]);   // 42
ио.вывести(данные["флаг"]);    // истина

// Тест 5: Установка элементов
ио.вывести("Тест 5: Установка элементов");
персона["имя"] = "Петр";
ио.вывести(персона["имя"]);    // "Петр"
персона["email"] = "petr@test.ru";
ио.вывести(персона["email"]);  // "petr@test.ru"
ио.вывести(длина(персона));    // 4

// Тест 6: Вложенные словари
ио.вывести("Тест 6: Вложенные словари");
пер адрес = {
    "город": "Москва",
    "координаты": {
        "широта": 55.75,
        "долгота": 37.61
    }
};
пер коорд = адрес["координаты"];
ио.вывести(коорд["широта"]);   // 55.75
ио.вывести(коорд["долгота"]);  // 37.61

// Тест 7: Несуществующий ключ возвращает ничто
ио.вывести("Тест 7: Несуществующий ключ");
пер значение = персона["несуществует"];
ио.вывести(значение);  // ничто

ио.вывести("=== Все тесты завершены ===");
