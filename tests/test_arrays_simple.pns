использовать "стд/ввод_вывод" как ио;

ио.вывести("=== Тест массивов ===");

// Тест 1: Пустой массив
ио.вывести("Тест 1: Пустой массив");
пер пустой = [];
ио.вывести(длина(пустой));  // 0

// Тест 2: Массив с числами
ио.вывести("Тест 2: Массив с числами");
пер числа = [1, 2, 3, 4, 5];
ио.вывести(длина(числа));   // 5

// Тест 3: Индексирование
ио.вывести("Тест 3: Индексирование");
ио.вывести(числа[0]);        // 1
ио.вывести(числа[2]);        // 3
ио.вывести(числа[4]);        // 5

// Тест 4: Массив со строками
ио.вывести("Тест 4: Массив со строками");
пер слова = ["привет", "мир", "ponos"];
ио.вывести(слова[0]);        // "привет"
ио.вывести(слова[1]);        // "мир"
ио.вывести(длина(слова));    // 3

// Тест 5: Массив со смешанными типами
ио.вывести("Тест 5: Смешанные типы");
пер смешанный = [1, "два", истина, 3.14];
ио.вывести(смешанный[0]);    // 1
ио.вывести(смешанный[1]);    // "два"
ио.вывести(смешанный[2]);    // истина
ио.вывести(смешанный[3]);    // 3.14

// Тест 6: Срезы массива
ио.вывести("Тест 6: Срезы");
пер срез1 = числа[1:4];      // [2, 3, 4]
ио.вывести(длина(срез1));    // 3
ио.вывести(срез1[0]);        // 2
ио.вывести(срез1[2]);        // 4

// Тест 7: Установка элементов
ио.вывести("Тест 7: Установка элементов");
числа[0] = 10;
ио.вывести(числа[0]);        // 10
числа[4] = 50;
ио.вывести(числа[4]);        // 50

// Тест 8: Вложенные массивы
ио.вывести("Тест 8: Вложенные массивы");
пер матрица = [[1, 2], [3, 4], [5, 6]];
ио.вывести(длина(матрица));  // 3
пер строка1 = матрица[0];
ио.вывести(строка1[0]);      // 1
ио.вывести(строка1[1]);      // 2

ио.вывести("=== Все тесты завершены ===");
